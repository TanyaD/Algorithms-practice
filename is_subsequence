/** Given a string s and a string t, check if s is subsequence of t.
*/

var isSubsequence = function(s, t) {
    var count = 0;
    for(var i = 0; i<t.length; i++){
        if(s[count] == t[i]){
            count++;
        }
    }
    if(count == s.length){
        return true;
    }
    else return false;
};
